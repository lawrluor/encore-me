{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/logout.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/users/me.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/acts/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/acts/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sets/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sets/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/songs/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/songs/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/acts/members.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/users/tree.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    },
    {
      "source": "/api/auth/logout",
      "destination": "/api/auth/logout.js"
    },
    {
      "source": "/api/users/me",
      "destination": "/api/users/me.js"
    },
    {
      "source": "/api/users/:id/tree",
      "destination": "/api/users/tree.js"
    },
    {
      "source": "/api/songs",
      "destination": "/api/songs/index.js"
    },
    {
      "source": "/api/songs/:id",
      "destination": "/api/songs/[id].js"
    },
    {
      "source": "/api/acts",
      "destination": "/api/acts/index.js"
    },
    {
      "source": "/api/acts/:id/members",
      "destination": "/api/acts/members.js"
    },
    {
      "source": "/api/acts/:id",
      "destination": "/api/acts/[id].js"
    },
    {
      "source": "/api/sets",
      "destination": "/api/sets/index.js"
    },
    {
      "source": "/api/sets/:id",
      "destination": "/api/sets/[id].js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}