{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    },
    {
      "source": "/api/users",
      "destination": "/api/users/index.js"
    },
    {
      "source": "/api/users/:id",
      "destination": "/api/users/[id].js"
    },
    {
      "source": "/api/songs",
      "destination": "/api/songs/index.js"
    },
    {
      "source": "/api/songs/delete-all",
      "destination": "/api/songs/delete-all.js"
    },
    {
      "source": "/api/songs/:id",
      "destination": "/api/songs/[id].js"
    },
    {
      "source": "/api/acts",
      "destination": "/api/acts/index.js"
    },
    {
      "source": "/api/acts/delete-all",
      "destination": "/api/acts/delete-all.js"
    },
    {
      "source": "/api/acts/:id/members",
      "destination": "/api/acts/members.js"
    },
    {
      "source": "/api/acts/:id",
      "destination": "/api/acts/[id].js"
    },
    {
      "source": "/api/users/delete-all",
      "destination": "/api/users/delete-all.js"
    },
    {
      "source": "/api/:path",
      "destination": "/api/:path.js"
    },
    {
      "source": "/",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}