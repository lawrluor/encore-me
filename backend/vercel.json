{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    },
    {
      "source": "/api/auth/logout",
      "destination": "/api/auth/logout.js"
    },
    {
      "source": "/api/users/me",
      "destination": "/api/users/me.js"
    },
    {
      "source": "/api/users/promoted-set",
      "destination": "/api/users/promoted-set.js"
    },
    {
      "source": "/api/users/:id",
      "destination": "/api/users/[id].js"
    },
    {
      "source": "/api/songs",
      "destination": "/api/songs/index.js"
    },
    {
      "source": "/api/songs/:id",
      "destination": "/api/songs/[id].js"
    },
    {
      "source": "/api/acts",
      "destination": "/api/acts/index.js"
    },
    {
      "source": "/api/acts/:id/members",
      "destination": "/api/acts/members.js"
    },
    {
      "source": "/api/acts/:id",
      "destination": "/api/acts/[id].js"
    },
    {
      "source": "/api/sets",
      "destination": "/api/sets/index.js"
    },
    {
      "source": "/api/sets/:id",
      "destination": "/api/sets/[id].js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}